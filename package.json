{
  "name": "@bromanla/logger",
  "type": "module",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "prebuild": "rm -rf build",
    "build": "tsc --build",
    "build:lib": "tsup src/index.ts --format esm --dts",
    "start": "node --env-file=.env build/index.js",
    "start:dev": "tsc-watch --onSuccess \"node --env-file=.env --inspect build/index.js\""
  },
  "devDependencies": {
    "@types/node": "^22.5.4",
    "prettier": "^3.3.3",
    "tsc-watch": "^6.2.0",
    "tsup": "^8.2.4",
    "typescript": "^5.6.2"
  },
  "imports": {
    "#src/*": "./build/*",
    "#root/*": "./"
  },
  "volta": {
    "node": "22.8.0",
    "npm": "10.8.2"
  }
}
